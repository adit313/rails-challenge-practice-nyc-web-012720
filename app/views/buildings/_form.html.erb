<%= form_for @building do |f| %>
<%= f.label :name, "Name"  %>
<%= f.text_field :name %>
<br>
<%= f.label :Country, "Country"  %>
<%= f.text_field :country %>
<br>
<%= f.label :address, "Address"  %>
<%= f.text_field :address %>
<br>
<%= f.label :rent_per_floor, "Rent Per Floor"  %>
<%= f.text_field :rent_per_floor %>
<br>
<%= f.label :number_of_floors, "Number of Floors"  %>
<%= f.text_field :number_of_floors %>
<br>
<%= f.submit %>
<% end %>